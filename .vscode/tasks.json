{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "ðŸ’¥ Force Push Full Project to GitHub",
      "type": "shell",
      "command": "cmd.exe",
      "args": [
        "/d",
        "/c",
        "git add . && git commit -m \"Full project update %date% %time%\" && git push --force"
      ],
      "problemMatcher": [],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "shared",
        "showReuseMessage": false,
        "clear": true
      },
      "options": {
        "cwd": "${workspaceFolder}",
        "shell": {
          "executable": "cmd.exe",
          "args": ["/d", "/c"]
        }
      }
    }
  ]
}
